}
.hx-navbar__content {
  display: block;
  position: relative;
  width: 100%;
  /*background-color: $uni-bg-color*/
  overflow: hidden;
}
.hx-navbar__content .navbgimg {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
  width: 100%;
}
.hx-navbar__content .hx-navbar__content_view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.hx-navbar__content .hx-status-bar {
  display: block;
  width: 100%;
  height: 40px;
  height: 25px;
  position: relative;
  z-index: 1;
}
.hx-navbar__header {
  position: relative;
  z-index: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  width: 100%;
  height: 44px;
  line-height: 44px;
  font-size: 36rpx;
