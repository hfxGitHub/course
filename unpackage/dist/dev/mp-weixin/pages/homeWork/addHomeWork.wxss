}
.hx-navbar--fixed {
  position: fixed;
  top: 0;
  z-index: 998;
}
.hx-navbar--shadow {
  box-shadow: 0 2rpx 12rpx #ccc;
}
.hx-navbar--border:after {
  position: absolute;
  z-index: 3;
  bottom: 0;
  left: 0;
  right: 0;
  height: 1px;
  content: '';
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  background-color: #efefef;
}
